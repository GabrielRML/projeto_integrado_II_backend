<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Usuario.
    -->
    <changeSet id="20240527215304-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="internal_user_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario__internal_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_user"
                                 />

        <addForeignKeyConstraint baseColumnNames="estado_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario__estado_id"
                                 referencedColumnNames="id"
                                 referencedTableName="estado"
                                 />

        <addForeignKeyConstraint baseColumnNames="cidade_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario__cidade_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cidade"
                                 />
    </changeSet>
</databaseChangeLog>
